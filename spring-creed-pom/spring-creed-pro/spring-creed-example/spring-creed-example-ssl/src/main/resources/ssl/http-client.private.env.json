{
  "ssl-dev": {
    "SSLConfiguration": {
      "verifyHostCertificate": false,
      "hasCertificatePassphrase": true,
      "clientCertificate": {
        "path": "./creed-mall-client.p12",
        "format": "P12"
      }
    }
  },
  "ssl-dev-pem": {
    "SSLConfiguration": {
      "verifyHostCertificate": false,
      "hasCertificatePassphrase": false,
      "clientCertificate": {
        "path": "./creed-mall-client.crt",
        "format": "PEM"
      },
      "clientCertificateKey": {
        "path": "./creed-mall-client.key",
        "format": "PEM"
      }
    }
  }
}